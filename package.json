{
  "name": "infura-explorer",
  "version": "1.2.1",
  "author": "Thomas Lipari <thomas.lipari@gmail.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/flamingYawn/infura-explorer"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "build:docker": "RAZZLE_PUBLIC_DIR=/public razzle build && docker build -t infura-explorer .",
    "test": "razzle test --env=jsdom",
    "lint": "tslint -p tsconfig.json",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/exenv": "^1.2.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.119",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-loadable": "^5.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/web3": "^1.0.18",
    "@types/webpack-env": "^1.13.6",
    "axios": "^0.21.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "exenv": "^1.2.2",
    "express": "^4.16.4",
    "moment": "^2.29.4",
    "morgan": "^1.9.1",
    "razzle": "^3.0.0",
    "razzle-plugin-typescript": "^2.4.0",
    "react": "16.8",
    "react-dom": "16.8",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "web3": "^1.0.0-beta.52"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "razzle-plugin-scss": "^2.2.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "tslint-config-standard": "^8.0.1"
  },
  "engines": {
    "node": "~10.13.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/test/test.config.ts"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
